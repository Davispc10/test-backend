      "email": "a"
    },
    "version": "1.0.0"
  },
  "paths": {

    "/sessions/": {
      "post": {
        "tags": ["Auth"],
        "summary": "Authenticate user",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },

    "/users": {
      "post": {
        "tags": ["Users"],
        "summary": "Register user",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "409": {
            "description": "Conflict"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/users/username/{username}": {
      "get": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": ["Users"],
        "summary": "Get user by username",
        "parameters": [{
          "name": "username",
          "in": "path",
          "required": true,
          "description": "Username to be found",
          "schema": {
            "type": "string"
          }
        }],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/users/email/{email}": {
      "get": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": ["Users"],
        "summary": "Get user by email",
        "parameters": [{
          "name": "email",
          "in": "path",
          "required": true,
          "description": "Email to be found",
          "schema": {
            "type": "string"
          }
        }],
        "responses": {
          "200": {
            "description": "OK"
          },
          "404": {
            "description": "Not Found"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/pokemons/": {
      "get": {
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": ["Pokemon"],
        "summary": "Get Pokemons",
        "parameters": [
          {
            "in": "query",
            "name": "page",
            "required": true,
            "schema": {
              "type": "number"
            }
          },{
            "in": "query",
            "name": "limit",
            "required": true,
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "name",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "pokedexNumber",
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "generation",
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "legendary",
            "schema": {
              "type": "number"
            }
          },
          {
            "in": "query",
            "name": "type1",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "weather1",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    }
  },

  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}
