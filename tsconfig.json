{
  "include": [
    "./src/"
  ],
  "compilerOptions": {
    "outDir": "./build",
    "target": "es2016",
    "lib": ["ESNext"],
    "sourceMap": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
